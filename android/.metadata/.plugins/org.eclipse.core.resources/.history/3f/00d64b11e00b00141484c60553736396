package com.hurix.epubRnD.Views;

import android.content.Context;
import android.util.AttributeSet;
import android.widget.RelativeLayout;

public class HelperViewForPageCount extends RelativeLayout 
{
	public HelperViewForPageCount(Context context) {
		super(context);
		// TODO Auto-generated constructor stub
	}

	public HelperViewForPageCount(Context context, AttributeSet attrs) {
		super(context, attrs);
		// TODO Auto-generated constructor stub
	}

	public HelperViewForPageCount(Context context, AttributeSet attrs,
			int defStyle) {
		super(context, attrs, defStyle);
		// TODO Auto-generated constructor stub
	}

	public abstract class PageCountListener 
	{
		public abstract void onPageCountComplete();
	}
}
